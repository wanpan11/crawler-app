{
  "name": "rt-rsbuild",
  "version": "1.0.1",
  "description": "react-ts 后台管理系统项目模版内置菜单路由、状态管理多环境打包等工具开箱即用",
  "main": "index.js",
  "type": "module",
  "keywords": [
    "react",
    "admin",
    "typescript"
  ],
  "author": "wanpan11",
  "license": "ISC",
  "homepage": "https://wanpan11.github.io/react-admin-tp/",
  "scripts": {
    "start": "cross-env NODE_ENV=development rsbuild dev --config ./scripts/rsbuild.config.ts --mode=development",
    "build": "cross-env NODE_ENV=production rsbuild build --config ./scripts/rsbuild.config.ts --mode=production",
    "dev:build": "cross-env NODE_ENV=development rsbuild build --config ./scripts/rsbuild.config.ts --mode=development",
    "lint": "eslint --fix --ext js,jsx,ts,tsx src/ && tsc --noEmit",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@rsbuild/core": "^1.2.19",
    "@rsbuild/plugin-less": "^1.1.1",
    "@rsbuild/plugin-react": "^1.1.1",
    "@rsdoctor/rspack-plugin": "^1.0.0",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^18.19.24",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "compression-webpack-plugin": "^11.1.0",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.6",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "mockjs": "^1.1.0",
    "prettier": "2.5.1",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.4",
    "@unocss/reset": "^0.53.6",
    "antd": "^5.15.3",
    "axios": "^1.6.8",
    "classnames": "^2.5.1",
    "mobx": "^6.12.0",
    "mobx-react-lite": "^3.4.3",
    "nanoid": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "swr": "^2.3.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "bash -c tsc --noEmit"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}